	<script>
let id = document.getElementById('invoiceNumber').value
		function init(id){
		

			  $.ajax({
                dataType: 'json',
                data: {
                    code: id,
                    
                },
                type: 'POST',
                url: "/clerk/autoInvoiceX",
                success: function(data) {
               
                
                
                 $("#fullname").val(data.studentName)
                 $("#email").val(data.studentEmail)
				 $("#address").val(data.studentAddress)
	
				 $("#mobile").val(data.studentMobile)
				 $("#balance").val(data.balance)
				 $("#uid").val(data.studentId)
				 $("#studentId2").val(data._id)
              
			loadItems(data.studentId)
                 
          
                }
               
            });
		}
		//console.log(document.getElementById('invoNumber').value)
